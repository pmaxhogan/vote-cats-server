{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "SUPERPRIVATEKEY.json"
    ],
    "rewrites": [
      {
        "source": "/api/**", "function": "cats"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
